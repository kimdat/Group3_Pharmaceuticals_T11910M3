@model PharmaceuticalsCompany.Models.ContactUs

@{
    ViewData["Title"] = "create";
}

<h2>create</h2>

<h4>ContactUs</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="create" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CName" class="control-label"></label>
                <input asp-for="CName" class="form-control" />
                <span asp-validation-for="CName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CPhone" class="control-label"></label>
                <input asp-for="CPhone" class="form-control" />
                <span asp-validation-for="CPhone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FeedBack" class="control-label"></label>
                <input asp-for="FeedBack" class="form-control" />
                <span asp-validation-for="FeedBack" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
@ViewBag.msg

<div>
    <a asp-action="Index">Back to List</a>
</div>

